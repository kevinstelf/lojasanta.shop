0e7\u00f5es<\/p>\r\n    <\/div>\r\n  <\/div>\r\n<\/div>\r\n\r\n<script src=\"https:\/\/cdnjs.cloudflare.com\/ajax\/libs\/jquery\/3.6.4\/jquery.min.js\"\r\n  integrity=\"sha512-pumBsjNRGGqkPzKHndZMaAG+bir374sORyzM3uulLV14lN5LyykqNk8eEeUlUkB3U0M4FApyaHraT65ihJhDpQ==\"\r\n  crossorigin=\"anonymous\" referrerpolicy=\"no-referrer\"><\/script>\r\n<script src=\"https:\/\/cdnjs.cloudflare.com\/ajax\/libs\/jquery.mask\/1.14.16\/jquery.mask.min.js\"\r\n  integrity=\"sha512-pHVGpX7F\/27yZ0ISY+VVjyULApbDlD0\/X0rgGbTqCE7WFW5MezNTWG\/dnhtbBuICzsd0WQPgpE4REBLv+UqChw==\"\r\n  crossorigin=\"anonymous\" referrerpolicy=\"no-referrer\"><\/script>\r\n  <script>\r\n    async function chatInteraction() {\r\n        async function delay(ms) {\r\n            return new Promise(resolve => setTimeout(resolve, ms));\r\n        }\r\n\r\n        function addCheckIcon() {\r\n            var svgString = `<svg id=\"checkIcon\" class=\"checkIcon\" xmlns=\"http:\/\/www.w3.org\/2000\/svg\" viewBox=\"0 0 38.626 24.684\" style=\"position: absolute;bottom: 0;right: 5px;\" height=\"20\" width=\"18\">\r\n                <g id=\"Grupo_1\" data-name=\"Grupo 1\" transform=\"translate(-708.9 -601.383)\">\r\n                <path id=\"Caminho_6\" data-name=\"Caminho 6\" d=\"M728.035,623.468l1.382,1.482,17.929-20.334\" transform=\"translate(-1.937 -1.117)\" fill=\"none\" stroke=\"#07c654\" stroke-linecap=\"round\" stroke-width=\"3\"><\/path>\r\n                <path id=\"Caminho_7\" data-name=\"Caminho 7\" d=\"M712.017,616.07l7.088,8.039,17.757-20.14\" transform=\"translate(-1 -0.469)\" fill=\"none\" stroke=\"#07c654\" stroke-linecap=\"round\" stroke-width=\"3\"><\/path>\r\n                <\/g>\r\n            <\/svg>`;\r\n\r\n            var tempContainer = document.createElement('div');\r\n            tempContainer.innerHTML = svgString;\r\n\r\n            var svgElement = tempContainer.firstChild;\r\n            return svgElement;\r\n        }\r\n\r\n        function loadingWave() {\r\n            return `\r\n                <div class=\"wave\">\r\n                    <div class=\"ball\"><\/div>\r\n                    <div class=\"ball\"><\/div>\r\n                    <div class=\"ball\"><\/div>\r\n                <\/div>\r\n            `;\r\n        }\r\n\r\n        function calculateElementWidth(text) {\r\n            const averageCharacterWidth = 8.6; \/\/ Valor aproximado da largura de um caractere em pixels\r\n            const screenWidth = window.innerWidth;\r\n            let calculatedWidth;\r\n\r\n            if (screenWidth < 567) {\r\n                calculatedWidth = screenWidth - 40;\r\n            } else {\r\n                const widthFromText = text.length * averageCharacterWidth;\r\n                calculatedWidth = Math.min(widthFromText, 684);\r\n            }\r\n\r\n            return calculatedWidth;\r\n        }\r\n\r\n        async function typeWriter(text, element) {\r\n            let i = 0;\r\n\r\n            function typeWriterHandler() {\r\n                if (i < text.length) {\r\n                    \/\/ Aqui trocamos `+=` para usar `innerHTML` corretamente, interpretando <br>\r\n                    element.innerHTML += text.charAt(i).replace(\/\\n\/g, \"<br\/>\");\r\n                    i++;\r\n                    setTimeout(typeWriterHandler, 20); \/\/ Controla a velocidade da anima\u00e7\u00e3o\r\n                }\r\n            }\r\n\r\n            typeWriterHandler();\r\n        }\r\n\r\n        async function addMediaInChat(mediaType, src) {\r\n            const delay = ms => new Promise(res => setTimeout(res, ms));\r\n\r\n            var textElement = document.createElement('div');\r\n            textElement.setAttribute(\"id\", \"chat-text\");\r\n            textElement.classList.add(\"textConversa\");\r\n\r\n            var mediaElement;\r\n            if (mediaType === 'image') {\r\n                mediaElement = document.createElement('img');\r\n                mediaElement.setAttribute(\"src\", src);\r\n                mediaElement.onload = () => mediaElement.classList.add(\"loaded\");\r\n            } else if (mediaType === 'audio') {\r\n                mediaElement = document.createElement('audio');\r\n                mediaElement.setAttribute(\"controls\", \"controls\");\r\n                mediaElement.setAttribute(\"autoplay\", \"autoplay\");\r\n                mediaElement.setAttribute(\"src\", src);\r\n            } else if (mediaType === 'video') {\r\n                mediaElement = document.createElement('iframe');\r\n                mediaElement.src = src;\r\n                mediaElement.controls = true;\r\n                mediaElement.muted = false;\r\n                mediaElement.height = 240;\r\n                mediaElement.width = 320;\r\n            }\r\n\r\n            var checkIcon = await addCheckIcon();\r\n\r\n            document.getElementById(\"chatbox\").appendChild(textElement);\r\n\r\n            textElement.innerHTML = loadingWave(); \/\/ Loading\r\n            await delay(2000);\r\n            textElement.querySelector('.wave').remove(); \/\/ Remove o loading\r\n            await delay(400);\r\n            textElement.appendChild(mediaElement);\r\n            textElement.appendChild(checkIcon);\r\n            textElement.style.width = \"fit-content\";\r\n\r\n            await delay(1000);\r\n            scrollDown();\r\n        }\r\n\r\n        async function addTextInChat(text) {\r\n            const delay = ms => new Promise(res => setTimeout(res, ms));\r\n            var textElement = document.createElement('div');\r\n            textElement.setAttribute(\"id\", \"chat-text\");\r\n            textElement.classList.add(\"textConversa\");\r\n\r\n            var p = document.createElement('p');\r\n            var checkIcon = await addCheckIcon();\r\n            var newWidth = calculateElementWidth(text);\r\n\r\n            document.getElementById(\"chatbox\").appendChild(textElement);\r\n\r\n            textElement.innerHTML = loadingWave(); \/\/ Loading\r\n            await delay(2000);\r\n            textElement.querySelector('.wave').remove(); \/\/ Remove o loading\r\n            textElement.style.width = newWidth + 'px'; \/\/ ajusta largura do container de texto\r\n\r\n            await delay(400);\r\n            textElement.appendChild(p); \/\/ Adiciona o elemento P\r\n            textElement.appendChild(checkIcon);\r\n\r\n            \/\/ Chamamos o typeWriter para animar o texto\r\n            typeWriter(text, p);\r\n\r\n            await delay(1000);\r\n            scrollDown();\r\n        }\r\n\r\n        async function addInputText(placeholder, storageName, func) {\r\n            var div = document.createElement('div');\r\n            div.setAttribute(\"id\", \"chat-user-input\");\r\n            div.setAttribute(\"class\", \"chat-user-input\");\r\n\r\n            var input = document.createElement('input');\r\n            input.setAttribute(\"id\", \"text\")\r\n            input.setAttribute(\"placeholder\", placeholder);\r\n            input.classList.add(\"inputStyle\");\r\n\r\n            div.appendChild(input);\r\n\r\n            var button = document.createElement('button');\r\n            button.classList.add(\"form-control\");\r\n            button.innerHTML = \"Enviar\";\r\n            button.onclick = function () {\r\n                var inputValue = input.value;\r\n                localStorage.setItem(storageName, inputValue);\r\n                input.disabled = true;\r\n                button.disabled = true;\r\n                func();\r\n            };\r\n\r\n            div.appendChild(button);\r\n\r\n            document.getElementById(\"chatbox\").appendChild(div);\r\n\r\n            await delay(500);\r\n            scrollDown();\r\n        }\r\n\r\n        async function createButton(buttonText, func, redirectUrl = null) {\r\n            var div = document.createElement('div');\r\n            div.setAttribute(\"class\", \"chat-user-button\");\r\n\r\n            var button = document.createElement('button');\r\n            button.classList.add(\"form-control\");\r\n            button.innerHTML = buttonText;\r\n            button.onclick = async function () {\r\n                button.disabled = true; \/\/ Desativa o bot\u00e3o ap\u00f3s o clique\r\n                if (func) await func(); \/\/ Chama a fun\u00e7\u00e3o passada como argumento\r\n                if (redirectUrl) {\r\n                    window.location.href = redirectUrl; \/\/ Redireciona\r\n                }\r\n            };\r\n\r\n            div.appendChild(button);\r\n\r\n            document.getElementById(\"chatbox\").appendChild(div);\r\n\r\n            await delay(500);\r\n            scrollDown();\r\n        }\r\n\r\n        async function scrollDown() {\r\n            var objDiv = document.getElementById(\"chatbox\");\r\n            objDiv.scrollTo({\r\n                top: objDiv.scrollHeight + 100,\r\n                behavior: \"smooth\"\r\n            });\r\n\r\n            window.scrollTo({\r\n                top: document.body.scrollHeight,\r\n                behavior: \"smooth\"\r\n            });\r\n        }\r\n\r\n      async function primeiroBloco() {\r\n        await addTextInChat(\"Algo extraordin\u00e1rio pode mudar sua vida agora! \ud83c\udf1e\ud83c\udf1f\");\r\n        await delay(2000);\r\n        await addTextInChat(\"S\u00e3o Miguel Arcanjo est\u00e1 pronto para trazer o milagre que voc\u00ea tanto espera... \ud83d\ude07\");\r\n        await delay(2000);\r\n        await addTextInChat(\"Durante 9 dias ele vai te guiar para voc\u00ea ter Prosperar, ter Cura e Liberta\u00e7\u00e3o de tudo que prende a sua Prosperidade e sua sa\u00fade!\");\r\n\r\n        await delay(2000);\r\n        await addMediaInChat('audio', 'http:\/\/saomiguel.shop\/wp-content\/uploads\/2024\/09\/or9-1.mp3');\r\n\r\n        await delay(10000);\r\n        await addTextInChat(\"\ud83d\udc49 Pronta para transformar sua vida? Coloque seu nome na novena agora e receba as b\u00ean\u00e7\u00e3os de S\u00e3o Miguel. \ud83d\ude4f\");\r\n\r\n        await delay(2000);\r\n        createButton(\"EU QUERO COLOCAR MEU NOME!\", segundoBloco);\r\n      }\r\n\r\n\r\n      async function segundoBloco() {\r\n        await delay(2000);\r\n        await addTextInChat(\"Como voc\u00ea se chama?\");\r\n\r\n        await addInputText('Digite seu nome', 'nome', terceiroBloco);\r\n      }\r\n\r\n      async function terceiroBloco() {\r\n        await delay(2000);\r\n        await addTextInChat(\"Seu nome ser\u00e1 colocado em nosso livro de ora\u00e7\u00f5es para a novena de 9 dias!!\");\r\n\r\n        await delay(2000);\r\n        await addTextInChat(\"Assim como colocamos o da Amanda, de Goi\u00e2nia.\");\r\n\r\n        await delay(2000);\r\n        await addTextInChat(\"Olha o \u00e1udio que ela nos enviou...\");\r\n\r\n        await delay(2000);\r\n        await addMediaInChat('audio', 'http:\/\/saomiguel.shop\/wp-content\/uploads\/2024\/09\/or2.mp3');\r\n\r\n        await delay(7000);\r\n        await addTextInChat(`Antes de eu colocar seu nome, me diga ${localStorage.getItem('nome')}...`);\r\n\r\n        await delay(2000);\r\n        await addTextInChat(`Voc\u00ea est\u00e1 enfrentando alguma dificuldade para qual gostaria de orar?\r\n\r\n        \ud83d\udc49 Problemas de Sa\u00fade\r\n        \ud83d\udc49 Problemas Financeiros\r\n        \ud83d\udc49 Problemas Familiares\r\n        \ud83d\udc49 Problemas com \u00c1lcool\r\n        \ud83d\udc49 Problemas com V\u00edcio\r\n        \ud83d\udc49 N\u00e3o estou bem...\r\n        `);\r\n\r\n        await addInputText('Digite aqui', 'problema', quartoBloco);\r\n      }\r\n\r\n      async function quartoBloco() {\r\n\r\n        await delay(2000);\r\n        await addTextInChat(\"Ent\u00e3o, vamos fazer uma ora\u00e7\u00e3o juntos bem r\u00e1pido, t\u00e1? \ud83d\ude4f\");\r\n\r\n        await delay(2000);\r\n        await addTextInChat(\"Essa ora\u00e7\u00e3o vai abrir o caminho para S\u00e3o Miguel interceder por voc\u00ea desde agora...\");\r\n\r\n        await delay(2000);\r\n        await addTextInChat(\"Aperte no v\u00eddeo abaixo e repita as palavras... \ud83d\udc47\");\r\n\r\n        await delay(2000);\r\n        await addMediaInChat('video', 'https:\/\/www.youtube-nocookie.com\/embed\/hJ995rEVxl4?si=tMHAh9VMFOpeSCKf&amp;controls=0');\r\n\r\n        await delay(15000);\r\n        await addTextInChat(\"S\u00e3o Miguel Arcanjo \u00e9 quem vai abrir as portas da sua vida para a prosperidade, cura e liberta\u00e7\u00e3o...\");\r\n\r\n        await addTextInChat(`${localStorage.getItem('nome')}, participando da Novena de S\u00e3o Miguel voc\u00ea ter\u00e1:\r\n\r\n        \u2705 Prote\u00e7\u00e3o espiritual e f\u00edsica \r\n        \u2705 Abund\u00e2ncia em todas as \u00e1reas\r\n        \u2705 Prosperidade financeira\r\n        \u2705 Sa\u00fade\r\n        \u2705 Escudo Espiritual para voc\u00ea e sua fam\u00edlia.\r\n        `);\r\n\r\n        await delay(2000);\r\n        await addTextInChat(`${localStorage.getItem('nome')}, \u00e9 disso que voc\u00ea precisa?`);\r\n\r\n        await delay(2000);\r\n        createButton(\"SIM, EU PRECISO!\", quintoBloco);\r\n      }\r\n\r\n      async function quintoBloco() {\r\n        await delay(2000);\r\n        await addTextInChat(\"Muitas pessoas j\u00e1 passaram pelo que voc\u00ea est\u00e1 passando exatamente agora, mas elas encontraram uma solu\u00e7\u00e3o...\");\r\n\r\n        await delay(2000);\r\n        await addTextInChat(\"Que foi o caso da Maria, que estava sofrendo com problemas financeiros.\");\r\n\r\n        await delay(2000);\r\n        await addMediaInChat('audio', 'http:\/\/saomiguel.shop\/wp-content\/uploads\/2024\/09\/or11.mp3');\r\n\r\n        await delay(14000);\r\n        await addMediaInChat('audio', 'http:\/\/saomiguel.shop\/wp-content\/uploads\/2024\/09\/or12.mp3');\r\n\r\n        await delay(5000);\r\n        await addTextInChat(\"Como voc\u00ea pode ver...\");\r\n\r\n        await delay(10000);\r\n        await addMediaInChat('audio', 'http:\/\/saomiguel.shop\/wp-content\/uploads\/2024\/09\/or4.mp3');\r\n\r\n        await delay(2000);\r\n        await addTextInChat(`${localStorage.getItem('nome')}, seja uma das poucas pessoas a receber esta b\u00ean\u00e7\u00e3o especial antes que acabe o tempo da Novena!`);\r\n\r\n        await delay(5000);\r\n        createButton(\"EU QUERO COME\u00c7AR A NOVENA HOJE!\", sextoBloco);\r\n      }\r\n\r\n      async function sextoBloco() {\r\n        await delay(2000);\r\n        await addTextInChat(\"Que maravilha!\");\r\n\r\n        await delay(2000);\r\n        await addTextInChat(\"O material especial que a Maria usou para atingir as gra\u00e7as tem tudo isso aqui:\");\r\n\r\n        await delay(2000);\r\n        await addTextInChat(`\u2714\ufe0f Guia completo de como fazer a Novena\r\n      \u2714\ufe0f Livros para Acompanhar\r\n      \u2714\ufe0f Sant\u00edssimo Sacramento\r\n      \u2714\ufe0f Missas do Dia 1 ao Dia 9\r\n      \u2714\ufe0f Ora\u00e7\u00f5es Di\u00e1rias Necess\u00e1rias\r\n      \u2714\ufe0f \u00c1udios de Ora\u00e7\u00f5es Dedicadas ao S\u00e3o Miguel Arcanjo\r\n      \u2714\ufe0f Medita\u00e7\u00f5es Guiadas\r\n      \u2714\ufe0f Dicas Pr\u00e1ticas \r\n      \u2714\ufe0f Caminho Espiritual\r\n      \u2714\ufe0f Como Prosperar Seguindo os Princ\u00edpios de Deus\r\n      \u2714\ufe0f Prote\u00e7\u00e3o Espiritual, Purifica\u00e7\u00e3o e Fortalecimento da F\u00e9.\r\n      `);\r\n\r\n        await delay(2000);\r\n        await addMediaInChat('audio', 'http:\/\/saomiguel.shop\/wp-content\/uploads\/2024\/09\/or5.mp3');\r\n\r\n        await delay(8000);\r\n        createButton(\"EU QUERO TUDO ISSO NA MINHA VIDA!\", setimoBloco);\r\n      }\r\n\r\n      async function setimoBloco() {\r\n        await delay(2000);\r\n        await addTextInChat(`${localStorage.getItem('nome')}, parab\u00e9ns pela sua decis\u00e3o!`);\r\n\r\n        await delay(2000);\r\n        await addMediaInChat('audio', 'http:\/\/saomiguel.shop\/wp-content\/uploads\/2024\/09\/or6.mp3');\r\n        await delay(18000);\r\n        await addMediaInChat('audio', 'http:\/\/saomiguel.shop\/wp-content\/uploads\/2024\/09\/or7.mp3');\r\n        await delay(15000);\r\n        await addMediaInChat('audio', 'http:\/\/saomiguel.shop\/wp-content\/uploads\/2024\/09\/or8.mp3');\r\n        await delay(10000);\r\n        await addMediaInChat('audio', 'http:\/\/saomiguel.shop\/wp-content\/uploads\/2024\/09\/or13.mp3');\r\n\r\n        await delay(2000);\r\n        await addTextInChat(\"Em 10 minutos, entrarei em contato com voc\u00ea pelo meu WhatsApp exclusivo, para tirar qualquer d\u00favida que voc\u00ea possa ter.\");\r\n\r\n        await delay(2000);\r\n        createButton(\"EU QUERO TRANSFORMAR MINHA VIDA!\", null, 'https:\/\/seguro.saomiguel.shop\/r\/6FWP3NZQJ0');\r\n      }\r\n\r\n      primeiroBloco();\r\n    }\r\n\r\n    chatInteraction();\r\n  <\/script>\r\n\r\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t","protected":false},"excerpt":{"rendered":"<p>WhatsApp Web S\u00e3o Miguel online Esta \u00e9 uma conta comercial e n\u00e3o recebe liga\u00e7\u00f5es<\/p>\n","protected":false},"author":1,"featured_media":0,"parent":0,"menu_order":0,"comment_status":"closed","ping_status":"closed","template":"elementor_canvas","meta":{"footnotes":""},"class_list":["post-4494","page","type-page","status-publish","hentry"],"_links":{"self":[{"href":"https:\/\/saomiguel.shop\/wp-json\/wp\/v2\/pages\/4494"}],"collection":[{"href":"https:\/\/saomiguel.shop\/wp-json\/wp\/v2\/pages"}],"about":[{"href":"https:\/\/saomiguel.shop\/wp-json\/wp\/v2\/types\/page"}],"author":[{"embeddable":true,"href":"https:\/\/saomiguel.shop\/wp-json\/wp\/v2\/users\/1"}],"replies":[{"embeddable":true,"href":"https:\/\/saomiguel.shop\/wp-json\/wp\/v2\/comments?post=4494"}],"version-history":[{"count":0,"href":"https:\/\/saomiguel.shop\/wp-json\/wp\/v2\/pages\/4494\/revisions"}],"wp:attachment":[{"href":"https:\/\/saomiguel.shop\/wp-json\/wp\/v2\/m